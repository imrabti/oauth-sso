<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to="myapp">
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.gwtplatform.dispatch.rest.DispatchRest'/>
    <inherits name='com.gwtplatform.mvp.MvpWithEntryPoint'/>

    <source path="client"/>
    <source path="shared"/>

    <extend-configuration-property name="gin.ginjector.modules" value="org.nuvola.myapp.client.ClientModule"/>
    <set-configuration-property name="gwtp.bootstrapper" value="org.nuvola.myapp.client.BootstrapperImpl"/>
    <set-configuration-property name="CssResource.legacy" value="true" />
</module>
